(core.define @read-int (core.lambda (@variable-symbol)
    (core.list
      (core.string.concat @"LOADA " ~(core.symbol.concat variable-symbol @: @address))
      @"CALL getint")))

(core.define @read-char (core.lambda (variable-symbol)
    (core.list
      (core.string.concat @"LOADA " ~(core.symbol.concat variable-symbol @: @address))
      @"CALL get"
      @"CALL geteol")))

