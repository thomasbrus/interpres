(core.define @<= (core.lambda (a b)
    (core.list a b @"CALL le")))

(core.define @< (core.lambda (a b)
    (core.list a b @"CALL lt")))

(core.define @== (core.lambda (a b)
    (core.list a b 1 @"CALL eq")))

(core.define @!= (core.lambda (a b)
    (core.list a b 1 @"CALL ne")))

(core.define @> (core.lambda (a b)
    (core.list a b @"CALL gt")))

(core.define @>= (core.lambda (a b)
    (core.list a b @"CALL ge")))

