(core.define @asm.loadl (core.lambda (quoted-integer)
    (core.list (core.string.concat
        @"LOADL " (core.integer-to-string quoted-integer)))))

(core.define @if (core.lambda (condition, consequent, (alternative @"LOAD nil"))
    (core.list @if condition consequent alternative)))

(if @true 1)
(if @false 1 2)

(core.define @const (core.lambda (@name, initial-value)
    (core.define name initial-value)))

(const hello 123)

hello

