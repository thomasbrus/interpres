(require "../../../preludes/minimal/jvm.interpres")

(var age)
(const initial-letter 'T')

(println-str "Please enter your age:")
(read-int age)

(if (>= age 21)
  (println-str "Access granted!")
  (println-str "Access denied."))

(assign age (+ age 4))

(println-int age)
(println-chr initial-letter)

