(core.define @+ (core.lambda (left, right)
    (core.list left right @("iadd"))))

(core.define @- (core.lambda (left, right)
    (core.list left right @("isub"))))

(core.define @* (core.lambda (left, right)
    (core.list left right @("imul"))))

(core.define @/ (core.lambda (left, right)
    (core.list left right @("idiv"))))

(core.define @% (core.lambda (left, right)
    (core.list left right @("irem"))))

