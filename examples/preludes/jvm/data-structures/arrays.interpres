(core.define @array core.list)

(core.define @array-size (core.lambda (identifier) (core.list (core.string.concat @"bipush " ~(core.symbol.concat identifier @: @size)))))

(core.define @nth (core.lambda (identifier index)
    (core.list.concat (asm.aload (core.string.concat ~(core.symbol.concat identifier @: @address))) index (core.list @"iaload"))))