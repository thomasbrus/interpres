(require "../../preludes/tam.interpres")



(var-array @ints (array 1 2 3))
(var-array @chars (array 'a' 'b' 'c' 'd'))
(var-array @bools (array true false true true))

(print-char (nth @chars 3))
(assign-nth @chars 3 'X')
(print-char (nth @chars 3))
(assign-array @ints (core.list 4 5 6))
(print-int (nth @ints 1))

(var @i 0)
(while (< i (array-size @ chars)) (do (if (nth @bools i) (print-char (nth @chars i))) (assign @i (+ i 1))))


